class Solution {
    public List<Integer> findMissingElements(int[] nums) {
       HashSet<Integer> hs = new HashSet<>();
       int mn = Integer.MAX_VALUE;
       int mx = Integer.MIN_VALUE;

       for(int num:nums){
        mn = Math.min(mn,num);
        mx = Math.max(mx,num);
       } 

       for(int num:nums){
        hs.add(num);
       }

       List<Integer> res = new ArrayList<>();
       for(int i =mn;i<=mx;i++){
        if(!hs.contains(i)){
            res.add(i);
        }
       }
       return res;
    }
}
